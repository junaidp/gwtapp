<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<h4 class="text-light blue">View Plan</h4>
		<div style="padding:4px">
			<g:CheckBox ui:field="checkBoxPlanType" styleName="inLine"> Plan Type</g:CheckBox>
			<div style="padding-left:6px">
				<g:CheckBox ui:field="checkBoxPaymentPlan" styleName="inLine" text="Payment Plan ("></g:CheckBox>
				<g:CheckBox ui:field="checkBoxMonthly" styleName="inLine">Monthly</g:CheckBox>
				<g:CheckBox ui:field="checkBoxQuaterly" styleName="inLine">Quartely</g:CheckBox>
				<g:CheckBox ui:field="checkBoxYearly" text="Yearly )"></g:CheckBox>
			</div>
		
		</div>
		<div style="padding:4px">
			<g:CheckBox ui:field="paymentDetails"> Payment Details</g:CheckBox>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 