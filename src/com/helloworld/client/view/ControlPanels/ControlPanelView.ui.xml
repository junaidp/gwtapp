<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	 <g:HTMLPanel>
		<div style="width:100% height:100% padding:10px">
			<div style="width: 300px; height: 300px;  background-color:#F5F5F5">
				<div class="center">
					<g:Image url="images/dashboard.png" ui:field="imageUrl"></g:Image>
				</div>
				<g:Label styleName="controlPanelHeadings centeredText hover blue" ui:field="heading"></g:Label>
			</div>
		</div>
	</g:HTMLPanel>
	
</ui:UiBinder> 