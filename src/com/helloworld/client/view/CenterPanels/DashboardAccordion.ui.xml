<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:M="urn:import:com.helloworld.client.view.MyDashboard">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<div>
			<g:StackLayoutPanel unit='PX' width="600px"
				height="600px">
				<g:stack>
					<g:header size='33'>
						<b>Configuration </b>
						
					</g:header>
					<M:ConfigurationView></M:ConfigurationView>
				</g:stack>
				<g:stack>
					<g:customHeader size='33'>
						<g:Label>My Account </g:Label>
					</g:customHeader>
					<M:MyAccountView></M:MyAccountView>
				</g:stack>
				
				<g:stack>
					<g:customHeader size='33'>
						<g:Label>Run Time </g:Label>
					</g:customHeader>
					<M:RunTimeView></M:RunTimeView>
				</g:stack>
				
				<g:stack>
					<g:customHeader size='33'>
						<g:Label>Reports </g:Label>
					</g:customHeader>
					<M:ReportsView></M:ReportsView>
				</g:stack>
			</g:StackLayoutPanel>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 