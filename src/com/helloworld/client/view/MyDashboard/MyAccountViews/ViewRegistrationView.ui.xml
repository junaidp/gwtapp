<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<div style="padding-left:70%">
	<g:Anchor ui:field="ancEdit" styleName="hover" text="Edit"> Edit</g:Anchor>
	</div>
		<div style="padding-left:10%">
			<h4 class="text-light blue">View Registration</h4>
			<g:HTMLPanel ui:field="panelFirstName">
				<div>
					<g:Label styleName="text-light blue inLine" text="First Name: "></g:Label>
					<g:TextBox styleName="input-control text inLine" ui:field="txtFirstName" enabled="false"></g:TextBox>
				</div>
			</g:HTMLPanel>

			<g:HTMLPanel ui:field="panelLastName">
				<div>
					<g:Label styleName="text-light blue inLine" text="Last Name: "></g:Label>
					<g:TextBox styleName="input-control text inLine" ui:field="txtLastName" enabled="false"></g:TextBox>
				</div>
			</g:HTMLPanel>

			<g:HTMLPanel ui:field="panelRegisteredTo">
				<div>
					<g:Label styleName="text-light blue inLine" text="Registered to: "></g:Label>
					<g:TextBox styleName="input-control text inLine" ui:field="txtRegisteredTo" enabled="false"></g:TextBox>
				</div>
			</g:HTMLPanel>

			<g:HTMLPanel ui:field="panelLastEdited">
				<div>
					<g:Label styleName="text-light blue inLine" text="Last Edited: "></g:Label>
					<g:Label styleName="text-light inLine" ui:field="lblLastEdited" ></g:Label>
				</div>
			</g:HTMLPanel>

		</div>
		
		<div style="padding:5px">
		<g:Button styleName="button primary" ui:field="btnUpdate"> update </g:Button>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 